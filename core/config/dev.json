{
  "app": {
    "title": "Polyblog App",
    "shortTitle": "Polyblog",
    "description": "Polyblog App System Description",
    "baseHref": "/",
    "analytics": "UA-103026201-1"
  },
  "modules": {
    "app-router": true,
    "firebase-mixin": true,
    "not-authorized-page": true,
    "not-found-page": true,
    "home-page": true,
    "default-theme": true
  },
  "theme": {
    "src": "core/modules/default-theme",
    "ignoreComponents": []
  },
  "fragments": {
    "home-page": "modules/home-page/page/home-page.html",
    "not-found-page": "modules/not-found-page/page/not-found-page.html",
    "not-authorized-page": "modules/not-authorized-page/page/not-authorized-page.html"
  },
  "routing": {
    "/": {
      "node": "home-page"
    },
    "/p": "home-page-2"
  },
  "httpCodes": {
    "not-found": "not-found-page",
    "not-authorized": "not-authorized-page"
  },
  "serviceWorker": {
    "globIgnores": [],
    "runtimeCaching": []
  }
}
